{
  "name": "educational-need",
  "datapackage_version": "1.0-beta.18",
  "title": "Educational Need",
  "description": "Educational Need reports special services and accommodations provided to students by school districts.",
  "version": "1.0",
  "licenses": [{
    "url": "http://opendatacommons.org/licenses/pddl/",
    "name": "Open Data Commons Public Domain",
    "version": "1.0",
    "id": "odc-pddl"
  }],
  "sources": [{
    "name": "ctsde",
    "web": ""
  }],
  "author":[{
    "name": "Jenna Daly",
    "email": "jdaly@ctdata.org",
    "web": "http://ctdata.org"
  }],
  "contributor": [],
  "resources": [{
    "path": "data/educational_need_2016-2017.csv",
    "format": "csv",
    "schema": {
      "fields": [{
          "name": "District",
          "type": "string",
          "dimension": false
        }, {
          "name": "FIPS",
          "type": "string",
          "dimension": false
        }, {
          "name": "Year",
          "type": "string",
          "dimension": false
        }, {
          "name": "Measure Type",
          "type": "string",
          "dimension": true,
          "constraints": {
			      "enum": [
		          "Number", 
		          "Percent"
		        ]
			    }
        }, {
          "name": "Variable",
          "type": "string",
          "dimension": true, 
          "constraints": {
		        "enum": [
              "Indicator of Educational Need"
            ]
			    }
        }, {
          "name": "Indicator of Educational Need",
          "type": "string",
          "dimension": true, 
          "constraints": {
		        "enum": [
              "Special Education",		          
              "English Language Learner", 
              "Eligible for Free or Reduced Price Lunch", 
              "Total Students Evaluated"
            ]
			    }
        }, {          
          "name": "Value",
          "type": "number",
          "dimension": false
        }]
      }
    }
  ],
  "last_modified": "08/08/2017",
  "ckan_extras": {
    "full_description": {
      "ckan_name": "Full Description",
      "value": "This dataset provides counts and percentages of students in each district that are considered to have indicators of educational need. These indicators include being eligible for Free or Reduced Price Meals, receiving Special Education services, or are English Language Learners. This dataset also provides the total number of students for each district. Charter Districts have been entered as individual districts.",
      "type": "string"
    },
    "suppression": {
      "ckan_name": "Suppression",
      "value": "Observations of 5 or fewer students have been suppressed.",
      "type": "string"
    },
    "technical_notes": {
      "ckan_name": "Technical Notes",
      "value": "As of 2017, CTData has deprecated the previous data source (data.ct.gov) and has replaced it with EdSight. These indicators come from the Public Student Enrollment data sets, filtered by Free/Reduced Price Meal Eligibility, Special Education Status, and English Learner Status.",
      "type": "string"
    },
    "socrata": {
      "ckan_name": "Socrata",
      "value": "False",
      "type": "string"
    },
    "socrata_url": {
      "ckan_name": "Socrata Dataset URL",
      "value": "",
      "type": "string"
    },
    "domain": {
      "ckan_name": "Domain",
      "value": "Education",
      "type": "string"
    },
    "subdomain": {
      "ckan_name": "Subdomain",
      "value": "Student Demographics",
      "type": "string"
    },
    "numerator": {
      "ckan_name": "Numerator",
      "value": "Number of students with a given educational need.",
      "type": "string"
    },    
    "denominator": {
      "ckan_name": "Denominator",
      "value": "Total number of students for selected district and school year.",
      "type": "string"
    },       
    "geography": {
      "ckan_name": "Geography",
      "value": "District",
      "type": "string",
      "constraints": {"enum": ["Town", "County", "Town/County", "District", "Other"]}
    },
    "frequency": {
      "ckan_name": "Frequency",
      "value": "Annual",
      "type": "string"
    },
    "years_in_catalog": {
      "ckan_name": "Years in Catalog",
      "value": ["2011-2012", "2012-2013", "2013-2014", "2014-2015", "2015-2016", "2016-2017"],
      "type": "array"
    },
    "latest_year": {
      "ckan_name": "Latest Year Available",
      "value": "2016-2017",
      "type": "string"
    },
    "disabled_views": {
      "ckan_name": "Disabled Views",
      "value": [],
      "type": "array",
      "constraints": {"enum": ["map", "table", "bar", "line"]}
    },
    "units": {
      "ckan_name": "Units",
      "value": {
        "Number": "",
        "Percent": "%"
      }
    },
    "expected_number_of_geographies": {
		  "ckan_name": "Expected Number of Unique Geographies",
		  "value": 204,
		  "type": "string"
		},  
    "default": {      
      "ckan_name": "Default",
			"value": {
				"Measure Type": "Percent",
		 		"District": "Harford School District",
		 		"Year": ["2014-2015", "2015-2016", "2016-2017"],
		 		"Variable": "Indicator of Educational Need", 
		 		"Indicator of Educational Need": ["Special Education", "English Language Learner", "Eligible for Free or Reduced Price Lunch"]
			},
		  	"type": "object"
    }  
  },
  "spot_checks": [
    {
      "type": "$lookup",
      "filter": {
        "District": "New Haven School District",
        "Year": "2011-2012",
		 		"Variable": "Indicator of Educational Need",
        "Indicator of Educational Need": "Special Education",        
        "Measure Type": "Number"
      },
      "expected": {
        "type": "$match",
        "number type": "int",
        "value": 2281
      } 
    }, 
    {
      "type": "$lookup",
      "filter": {
